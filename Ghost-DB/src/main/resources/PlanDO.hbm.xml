<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ghost.domain.plan.PlanDO" table="PLAN">
		<id name="planId" type="int">
			<column name="PLANID" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<property name="plan" type="string">
			<column name="PLAN" length="4000"></column>
		</property>
	</class>
</hibernate-mapping>