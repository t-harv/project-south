<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ghost.domain.shape.ShapeDO" table="SHAPE">
		<id name="shapeId" type="int">
			<column name="SHAPEID" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<property name="shape" type="string">
			<column name="SHAPE" length="4000"></column>
		</property>
	</class>
</hibernate-mapping>