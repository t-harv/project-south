<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ghost.domain.edit.EditDO" table="EDIT">
		<id name="editId" type="int">
			<column name="EDITID" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<property name="edit" type="string">
			<column name="EDIT" length="4000"></column>
		</property>
	</class>
</hibernate-mapping>