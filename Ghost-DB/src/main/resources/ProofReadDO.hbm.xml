<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ghost.domain.proofread.ProofReadDO" table="PROOFREAD">
		<id name="proofReadId" type="int">
			<column name="PROOFREADID" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<property name="proofRead" type="string">
			<column name="PROOFREAD" length="4000"></column>
		</property>
	</class>
</hibernate-mapping>